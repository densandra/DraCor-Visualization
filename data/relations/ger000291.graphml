        </node>
        <node id="aegisth">
            <data key="label">Aegisth</data>
            <data key="gender">MALE</data>
            <data key="person-group">false</data>
        </node>
        <node id="pelopia">
            <data key="label">Pelopia</data>
            <data key="gender">FEMALE</data>
            <data key="person-group">false</data>
        </node>
        <node id="thyest">
            <data key="label">Thyest</data>
            <data key="gender">MALE</data>
            <data key="person-group">false</data>
        </node>
        <edge id="1" directed="false" source="thyest" target="atreus">
