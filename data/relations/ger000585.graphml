<?xml version="1.0" encoding="UTF-8"?>
<graphml xmlns="http://graphml.graphdrawing.org/xmlns">
    <key attr.name="label" attr.type="string" for="node" id="label"/>
    <key attr.name="Relation" attr.type="string" for="edge" id="relation"/>
    <key attr.name="Gender" attr.type="string" for="node" id="gender"/>
    <key attr.name="Person group" attr.type="boolean" for="node" id="person-group"/>
    <graph edgedefault="undirected">
        <node id="ester">
            <data key="label">Ester</data>
            <data key="gender">FEMALE</data>
            <data key="person-group">false</data>
        </node>
        <node id="moses">
            <data key="label">Moses</data>
            <data key="gender">MALE</data>
            <data key="person-group">false</data>
        </node>
        <node id="hirsch">
            <data key="label">Hirsch</data>
            <data key="gender">MALE</data>
            <data key="person-group">false</data>
        </node>
        <node id="rebekke">
            <data key="label">Rebekke</data>
            <data key="gender">FEMALE</data>
            <data key="person-group">false</data>
        </node>
        <node id="sturm">
            <data key="label">Korporal Sturm</data>
            <data key="gender">MALE</data>
            <data key="person-group">false</data>
        </node>
        <edge id="1" directed="false" source="hirsch" target="rebekke">
            <data key="relation">parent_of</data>
        </edge>
        <edge id="2" directed="false" source="ester" target="hirsch">
            <data key="relation">associated_with</data>
        </edge>
        <edge id="3" directed="false" source="moses" target="ester">
            <data key="relation">related_with</data>
        </edge>
    </graph>
</graphml>